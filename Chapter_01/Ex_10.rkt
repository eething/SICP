#lang racket

(define (A x y)
  (cond ((= y 0) 0)
        ((= x 0) (* 2 y))
        ((= y 1) 2)
        (else (A (- x 1) (A x (- y 1))))))


(A 1 10)
;1024 = 2^10
(A 2 4)
;65536 = 2^(2^(2^2)) = 2^(2^4) = 2^16
(A 3 3)
;65536
;= (A 2 (A 3 2))
;= (A 2 (A 2 (A 3 1)))
;= (A 2 (A 2 2))
;= (A 2 4)
;= 2^16

(define (f n) (A 0 n))
;f(n)= 2n
(f 2);4
(f 3);6
(f 4);8

(define (g n) (A 1 n))
;g(n)= 2^n
(g 2);4
(g 3);8
(g 4);16

(define (h n) (A 2 n))
;k(n)= 2^2^2^....^2
(h 2);4
(h 3);16
(h 4);65536

(define (k n) (* 5 n n))
;k(n)=5n^2
(k 2);20
(k 3);45
(k 4);80